{
  "event_log": {
    "_source": {
      "enabled": true
    },
    "_all": {
      "enabled": false
    },
    "_id": {
      "path": "id"
    },
    "properties": {
      "id": {
        "type": "string",
        "index": "not_analyzed"
      },
      "eventType": {
        "type": "string",
        "index": "not_analyzed"
      },
      "path": {
        "type": "string",
        "index": "not_analyzed"
      },
      "message": {
        "type": "string"
      },
      "createdBy": {
        "type": "string",
        "index": "not_analyzed"
      },
      "createdAt": {
        "type": "date"
      }
    }
  }
}
